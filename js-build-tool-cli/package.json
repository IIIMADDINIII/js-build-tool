{
  "name": "@iiimaddiniii/js-build-tool-cli",
  "version": "0.7.7-8",
  "description": "Template for creating a NodeJs package",
  "bin": {
    "js-build-tool": "./dist/index.js"
  },
  "type": "commonjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "clean": "pnpm run gulp clean",
    "build": "pnpm run gulp build",
    "buildCi": "pnpm run gulp buildCi",
    "gulp": "pnpm --package=@iiimaddiniii/js-build-tool@0.7.6 dlx js-build-tool",
    "test": "pnpm --package=@iiimaddiniii/js-build-tool-cli@0.7.7-5 dlx js-build-tool build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIIMADDINIII/js-build-tool-cli"
  },
  "author": "IIIMADDINIII <martin@elend.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IIIMADDINIII/js-build-tool-cli/issues"
  },
  "homepage": "https://github.com/IIIMADDINIII/js-build-tool-cli#readme",
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/js-build-tool-types": "npm:@iiimaddiniii/js-build-tool-types@^0.7.6",
    "@types/proper-lockfile": "^4.1.4"
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "execa": "^8.0.1",
    "proper-lockfile": "^4.1.2"
  }
}
