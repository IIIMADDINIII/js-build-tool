{
  "name": "@iiimaddiniii/js-build-tool",
  "version": "0.6.4",
  "description": "Tool for building my javascript projects",
  "bin": {
    "js-build-tool": "./dist/cli.cjs"
  },
  "exports": {
    "./cli": {
      "require": {
        "types": "./dist/cli.d.cts",
        "default": "./dist/cli.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "pnpm run gulp clean",
    "build": "pnpm run gulp build",
    "buildCi": "pnpm run gulp buildCi",
    "gulp": "pnpm --package=@iiimaddiniii/js-build-tool@0.6.3 dlx js-build-tool",
    "test": "pnpm --package=@iiimaddiniii/js-build-tool@0.6.3 dlx js-build-tool build",
    "testCi": "pnpm --package=@iiimaddiniii/js-build-tool@0.6.3 dlx js-build-tool buildCi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIIMADDINIII/node-package-template"
  },
  "author": "IIIMADDINIII <martin@elend.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IIIMADDINIII/node-package-template/issues"
  },
  "enginesToInstall": {
    "node": "18.0.0",
    "pnpm": "^7"
  },
  "homepage": "https://github.com/IIIMADDINIII/node-package-template#readme",
  "devDependencies": {
    "@electron-forge/core": "^7.3.0",
    "@electron-forge/maker-wix": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@microsoft/api-extractor": "^7.40.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "@schemastore/package": "^0.0.10",
    "@types/gulp": "^4.0.17",
    "@types/js-build-tool-types": "npm:@iiimaddiniii/js-build-tool-types@^0.5.6",
    "@types/node": "^20.11.20",
    "@types/semver": "^7.5.8",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fetch-github-release": "^1.0.0",
    "gulp": "^4.0.2",
    "rollup": "^4.12.0",
    "rollup-plugin-consts": "^1.2.0",
    "rollup-plugin-include-sourcemaps": "^0.7.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "@microsoft/api-extractor@7.40.6": "patches/@microsoft__api-extractor@7.40.6.patch"
    }
  }
}
