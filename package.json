{
  "name": "@iiimaddiniii/js-build-tool",
  "version": "0.0.15",
  "description": "Tool for building my javascript projects",
  "bin": {
    "js-build-tool": "./dist/cli.js"
  },
  "exports": {
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./gulp": {
      "types": "./dist/gulp.d.ts",
      "import": "./dist/gulp.js"
    },
    "./execa": {
      "types": "./dist/execa.d.ts",
      "import": "./dist/execa.js"
    },
    "./rimraf": {
      "types": "./dist/rimraf.d.ts",
      "import": "./dist/rimraf.js"
    }
  },
  "type": "module",
  "scripts": {
    "clean": "pnpm run gulp clean",
    "build": "pnpm run gulp build",
    "buildCi": "pnpm run gulp buildCi",
    "test": "pnpm run gulp build",
    "gulp": "pnpm --package=@iiimaddiniii/js-build-tool@0.0.13 dlx js-build-tool"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIIMADDINIII/node-package-template"
  },
  "author": "IIIMADDINIII <martin@elend.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IIIMADDINIII/node-package-template/issues"
  },
  "homepage": "https://github.com/IIIMADDINIII/node-package-template#readme",
  "devDependencies": {
    "@types/node": "^20.2.6",
    "@types/gulp": "^4.0.11",
    "typescript": "^5.1.3",
    "rollup": "^3.24.1",
    "rollup-config-iiimaddiniii": "^0.2.4",
    "tslib": "^2.5.3",
    "gulp": "^4.0.2",
    "gulp-execa": "^5.0.0",
    "rimraf": "^5.0.1"
  },
  "dependencies": {},
  "rollup": {
    "allowDevDependencies": true,
    "externalPackages": [
      "gulp",
      "@iiimaddiniii/js-build-tool"
    ],
    "resolveNode": true
  }
}
