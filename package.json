    "test": "pnpm --package=@iiimaddiniii/js-build-tool@0.7.2 dlx js-build-tool build",
    "testCi": "pnpm --package=@iiimaddiniii/js-build-tool@0.7.2 dlx js-build-tool buildCi"
  },
  "repository": {
