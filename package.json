{
  "name": "@iiimaddiniii/js-build-tool",
  "version": "0.1.3",
  "description": "Tool for building my javascript projects",
  "bin": {
    "js-build-tool": "./dist/cli.js"
  },
  "exports": {
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "clean": "pnpm run gulp clean",
    "build": "pnpm run gulp build",
    "buildCi": "pnpm run gulp buildCi",
    "test": "pnpm run gulp test",
    "gulp": "pnpm --package=@iiimaddiniii/js-build-tool@0.1.3 dlx js-build-tool"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIIMADDINIII/node-package-template"
  },
  "author": "IIIMADDINIII <martin@elend.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IIIMADDINIII/node-package-template/issues"
  },
  "homepage": "https://github.com/IIIMADDINIII/node-package-template#readme",
  "devDependencies": {
    "@types/node": "^20.2.6",
    "@types/gulp": "^4.0.11",
    "typescript": "^5.1.3",
    "rollup": "^3.24.1",
    "rollup-config-iiimaddiniii": "^0.2.4",
    "tslib": "^2.5.3",
    "gulp": "^4.0.2",
    "gulp-execa": "^5.0.0",
    "rimraf": "^5.0.1",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/pluginutils": "^5.0.2",
    "rollup-plugin-consts": "^1.1.0",
    "rollup-plugin-include-sourcemaps": "^0.7.0",
    "@schemastore/package": "^0.0.8"
  },
  "dependencies": {},
  "rollup": {
    "allowDevDependencies": true,
    "externalPackages": [
      "@iiimaddiniii/js-build-tool",
      "typescript",
      "rollup",
      "tslib",
      "gulp",
      "gulp-execa",
      "rimraf",
      "@rollup/plugin-commonjs",
      "@rollup/plugin-node-resolve",
      "@rollup/plugin-terser",
      "@rollup/plugin-typescript",
      "@rollup/plugin-json",
      "@rollup/pluginutils",
      "rollup-plugin-consts",
      "rollup-plugin-include-sourcemaps"
    ],
    "resolveNode": true,
    "doNotGenerateDeclarationInProduction": true
  }
}